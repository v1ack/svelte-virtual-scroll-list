{
  "name": "svelte-virtual-scroll-list",
  "description": "Svelte lib for virtualizing lists",
  "author": {
    "name": "v1ack",
    "url": "https://github.com/v1ack"
  },
  "keywords": [
    "svelte",
    "virtual",
    "virtual-list",
    "virtual-scroll"
  ],
  "version": "1.0.8",
  "scripts": {
    "build": "rollup -c rollup/rollup.config.js",
    "build:dev": "rollup -c rollup/rollup.config.dev.js",
    "dev": "rollup -c rollup/rollup.config.dev.js -w",
    "start:dev": "sirv example/public --no-clear"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v1ack/svelte-virtual-scroll-list.git"
  },
  "files": [
    "lib",
    "src",
    "types"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "sveld": "^0.7.1",
    "svelte": "^3.0.0",
    "svelte-check": "^1.5.4",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  },
  "svelte": "./src/index.js",
  "module": "./lib/index.mjs",
  "types": "./types/index.d.ts"
}
